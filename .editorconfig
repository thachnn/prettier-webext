# https://editorconfig.org
root = true

[*]
charset = utf-8
indent_style = space
indent_size = 2
end_of_line = lf
#
insert_final_newline = true
trim_trailing_whitespace = true
max_line_length = 100
quote_type = single

# noinspection EditorConfigValueCorrectness
[{.idea,node_modules,.yarn,dist}/**]
end_of_line = off
insert_final_newline = off
trim_trailing_whitespace = false
